{
  "name": "ngx-ui-scroll-app",
  "version": "0.0.0",
  "description": "Angular Demo App for ngx-ui-scroll",
  "main": "./bundles/ngx-ui-scroll.umd.js",
  "module": "./fesm5/ngx-ui-scroll.js",
  "es2015": "./fesm2015/ngx-ui-scroll.js",
  "esm5": "./esm5/ngx-ui-scroll.js",
  "esm2015": "./esm2015/ngx-ui-scroll.js",
  "fesm5": "./fesm5/ngx-ui-scroll.js",
  "fesm2015": "./fesm2015/ngx-ui-scroll.js",
  "typings": "./ngx-ui-scroll.d.ts",
  "scripts": {
    "start": "npm-run-all --parallel start-api-server start-dev-server",
    "start-dev-server": "ng serve --proxy-config proxy.conf.json",
    "start-api-server": "node ./server/index.js",
    "start-env": "node env",
    "lint": "tslint -c tslint.json  --project . ",
    "test": "npm run lint && karma start",
    "build-app": "ng build --prod --aot --source-map",
    "deploy-app": "npm run build-app && firebase deploy",
    "preinstall": "cd server && npm install",
    "postinstall": "npm run build-app",
    "pack:install": "npm run build && npm pack ./dist && npm install ngx-ui-scroll-1.6.3.tgz --no-save",
    "pack:start": "npm run pack:install && npm start",
    "build": "node build.js",
    "publish:lib": "npm run build && npm publish ./dist",
    "publish:lib:next": "npm run build && npm publish --tag next ./dist"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "9.1.7",
    "@angular/common": "9.1.7",
    "@angular/core": "9.1.7",
    "@angular/forms": "9.1.7",
    "@angular/platform-browser": "9.1.7",
    "@angular/platform-browser-dynamic": "9.1.7",
    "@angular/router": "9.1.7",
    "ngx-bootstrap": "^5.6.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "peerDependencies": {
    "rxjs": ">= 5.5.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.6",
    "@angular/cli": "~9.1.6",
    "@angular/compiler": "9.1.7",
    "@angular/compiler-cli": "9.1.7",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular2-template-loader": "0.6.2",
    "chalk": "2.4.2",
    "codelyzer": "5.2.2",
    "core-js": "3.1.3",
    "css-loader": "2.1.1",
    "dotenv": "^8.2.0",
    "istanbul-instrumenter-loader": "3.0.1",
    "jasmine-core": "~3.5.0",
    "karma": "^5.0.5",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "2.0.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "4.0.2",
    "npm-run-all": "^4.1.3",
    "raw-loader": "1.0.0",
    "reflect-metadata": "0.1.13",
    "rollup": "1.12.5",
    "rollup-plugin-license": "^2.0.1",
    "rollup-plugin-node-resolve": "5.0.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "sass-loader": "7.1.0",
    "shelljs": "0.8.3",
    "source-map-loader": "0.2.4",
    "ts-loader": "6.0.1",
    "tsickle": "0.38.1",
    "tslint": "^5.19.0",
    "tslint-angular": "^3.0.2",
    "typescript": "3.8.2",
    "uglify-js": "3.6.0",
    "webpack": "^4.43.0",
    "webpack-filter-warnings-plugin": "1.2.1"
  }
}
